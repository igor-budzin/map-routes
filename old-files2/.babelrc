{
	"presets": [
		"react",
		"es2015"
	],
	"plugins": [
		"transform-class-properties",
		"transform-decorators",
		"transform-object-rest-spread",
		"transform-runtime",
		["import", [{"libraryName": "antd", "style" : true}]]
	],
	"env": {
			"node": {
				"plugins": [
						[
								"babel-plugin-transform-require-ignore",
								{
										"extensions": [".less", ".css", "scss"]
								}
						],
						"react-hot-loader/babel"
				]
			}
		}
}